-- Safari
if app_id is equal to "com.apple.Safari" then
	tell application "Safari"
		set original_window to front window
		set tab_idx to index of current tab of original_window
	
		make new document
		move tabs (tab_idx - 1) thru 0 of original_window to front window
		close first tab of front window
	end tell

-- Chrome
else if app_id is equal to "com.google.Chrome" then
	tell application "Google Chrome"

		set original_window to front window
		set tab_idx to active tab index of original_window
	
		if tab_idx &gt; 1 then
			make new window
			repeat with i from 1 to (tab_idx - 1)
				set theURL to URL of tab i of original_window
				close tab i of original_window
				tell front window to make new tab with properties {URL:theURL}
			end repeat
			close tab 1 of front window
		end if
	end tell

end if</string>
