-- Safari
if app_id is equal to "com.apple.Safari" then

	tell application "Safari"
		tell front window
			set _old_tab to current tab
			set _new_tab to make new tab at after _old_tab
			set theURL to URL of _old_tab
			set current tab to _new_tab
			set URL of current tab to {theURL}
		end tell
	end tell

-- Chrome
else if app_id is equal to "com.google.Chrome" then

	tell application "Google Chrome" 
		tell front window
			set theURL to URL of active tab
			make new tab at after (get active tab) with properties {URL:theURL}
		end tell
	end tell

end if</string>
